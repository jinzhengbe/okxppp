# OKX API配置
OKX_REST_API_URL=https://www.okx.com
OKX_WS_API_URL=wss://wsaws.okx.com:8443/ws/v5/public
API_KEY=your_api_key_here
SECRET_KEY=your_secret_key_here
PASSPHRASE=your_passphrase_here

# 交易配置
TRADE_SYMBOL=BTC-USDT
TRADE_AMOUNT=0.001
BUY_THRESHOLD=30000
SELL_THRESHOLD=32000

# 风险控制
MAX_TRADES_PER_DAY=5
STOP_LOSS_PERCENTAGE=2

# 数据收集配置
COLLECT_SYMBOLS=BTC-USDT,ETH-USDT,SOL-USDT,XRP-USDT,DOGE-USDT

# InfluxDB配置
INFLUXDB_URL=http://localhost:8086
INFLUXDB_TOKEN=my-super-secret-auth-token
INFLUXDB_ORG=crypto
INFLUXDB_BUCKET=crypto_data

# PostgreSQL配置
POSTGRES_HOST=localhost
POSTGRES_PORT=5432
POSTGRES_DB=crypto_trading
POSTGRES_USER=admin
POSTGRES_PASSWORD=password 